
{{#if src}}
    {{#if isVideo}}
        <img class="img-thumbnail file" data-action="viewAttachment" title="{{title}}" alt="{{title}}" src="data:image/png;base64," />
    {{else}}
        <img class="img-thumbnail file" data-action="viewAttachment" title="{{title}}" alt="{{title}}" src="{{src}}">
    {{/if}}
    {{#if showOverlay}}
        <div class="file-overlay">
            <span data-action="trashAttachment" class="crafter-social-icon cs-icon-trash"></span>
        </div>
    {{/if}}
{{/if}}
